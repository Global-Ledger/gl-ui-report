{
  "preset": "ts-jest/presets/js-with-ts",
  "watchPathIgnorePatterns": [".*\\.json"],
  "collectCoverage": true,
  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  },
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/dist/"
  ],
  "testEnvironment": "node",
  "verbose": false,
  "bail": 1
}